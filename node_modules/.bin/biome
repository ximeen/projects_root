#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ximenin/Repositories/projects/projects_root/node_modules/.pnpm/@biomejs+biome@2.2.6/node_modules/@biomejs/biome/bin/node_modules:/home/ximenin/Repositories/projects/projects_root/node_modules/.pnpm/@biomejs+biome@2.2.6/node_modules/@biomejs/biome/node_modules:/home/ximenin/Repositories/projects/projects_root/node_modules/.pnpm/@biomejs+biome@2.2.6/node_modules/@biomejs/node_modules:/home/ximenin/Repositories/projects/projects_root/node_modules/.pnpm/@biomejs+biome@2.2.6/node_modules:/home/ximenin/Repositories/projects/projects_root/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ximenin/Repositories/projects/projects_root/node_modules/.pnpm/@biomejs+biome@2.2.6/node_modules/@biomejs/biome/bin/node_modules:/home/ximenin/Repositories/projects/projects_root/node_modules/.pnpm/@biomejs+biome@2.2.6/node_modules/@biomejs/biome/node_modules:/home/ximenin/Repositories/projects/projects_root/node_modules/.pnpm/@biomejs+biome@2.2.6/node_modules/@biomejs/node_modules:/home/ximenin/Repositories/projects/projects_root/node_modules/.pnpm/@biomejs+biome@2.2.6/node_modules:/home/ximenin/Repositories/projects/projects_root/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@biomejs+biome@2.2.6/node_modules/@biomejs/biome/bin/biome" "$@"
else
  exec node  "$basedir/../.pnpm/@biomejs+biome@2.2.6/node_modules/@biomejs/biome/bin/biome" "$@"
fi
